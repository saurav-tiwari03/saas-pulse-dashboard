{
  "name": "backend-api",
  "version": "1.0.0",
  "description": "Backend server for managing frontend store and admin dashboard",
  "main": "build/index.js",
  "scripts": {
    "start": "node build/index.js",
    "dev": "NODE_ENV=development nodemon --exec ts-node src/index.ts",
    "prod": "NODE_ENV=production node build/index.js",
    "build": "tsc",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:prod": "NODE_ENV=production prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:push": "prisma db push",
    "prisma:seed": "ts-node prisma/seed.ts"
  },
